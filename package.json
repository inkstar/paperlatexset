  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:web\" \"npm:dev:server\"",
    "dev:web": "vite",
    "dev:server": "DOTENV_CONFIG_PATH=.env.server tsx --tsconfig tsconfig.server.json watch server/src/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "preview:full": "concurrently \"npm:preview\" \"npm:server:start\"",
    "smoke": "bash scripts/smoke.sh",
    "smoke:auth": "bash scripts/smoke-auth.sh",
    "smoke:auth:matrix": "bash scripts/smoke-auth-matrix.sh",
    "smoke:auth:full": "bash scripts/smoke-auth-full.sh",
    "smoke:auth:code": "bash scripts/smoke-auth-code.sh",
    "smoke:compose": "bash scripts/smoke-compose.sh",
    "smoke:upload": "bash scripts/smoke-upload.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "db:bootstrap": "DOTENV_CONFIG_PATH=.env.server node -r dotenv/config scripts/bootstrap-db.mjs",
    "db:clean:demo": "DOTENV_CONFIG_PATH=.env.server node -r dotenv/config scripts/clean-demo-data.mjs",
    "db:seed:demo": "DOTENV_CONFIG_PATH=.env.server node -r dotenv/config scripts/seed-demo-data.mjs",
    "server:start": "DOTENV_CONFIG_PATH=.env.server tsx --tsconfig tsconfig.server.json server/src/index.ts"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@prisma/client": "https://registry.npmjs.org/@prisma/client/-/client-6.5.0.tgz",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "cors": "^2.8.5",
    "docx": "^9.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.555.0",
    "minio": "^8.0.6",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
